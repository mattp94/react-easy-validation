{
  "name": "react-easy-validation",
  "version": "2.0.0-alpha.1",
  "description": "A simple form validator for React",
  "keywords": [
    "react",
    "validation",
    "validate",
    "validator",
    "form"
  ],
  "main": "dist/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "jest",
    "start": "webpack --watch --config webpack.dev.js",
    "build": "webpack --config webpack.prod.js",
    "prepublishOnly": "npm test && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mattp94/react-easy-validation.git"
  },
  "author": "Matthieu Puibaraud <matthieu.puibaraud@isep.fr> (https://github.com/mattp94)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mattp94/react-easy-validation/issues"
  },
  "homepage": "https://github.com/mattp94/react-easy-validation#readme",
  "devDependencies": {
    "babel-loader": "^7.1.5",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "clean-webpack-plugin": "^0.1.19",
    "coveralls": "^3.0.2",
    "enzyme": "^3.6.0",
    "enzyme-adapter-react-16": "^1.5.0",
    "jest": "^23.5.0",
    "react": "^16.5.0",
    "react-dom": "^16.5.0",
    "react-test-renderer": "^16.5.0",
    "uglifyjs-webpack-plugin": "^1.3.0",
    "webpack": "^4.17.2",
    "webpack-cli": "^3.1.0",
    "webpack-merge": "^4.1.4"
  },
  "peerDependencies": {
    "react": "^0.14.0 || ^15.0.0 || ^16.0.0"
  },
  "dependencies": {
    "prop-types": "^15.6.2",
    "shallowequal": "^1.1.0"
  },
  "jest": {
    "verbose": true,
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.js"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 100,
        "functions": 100,
        "lines": 100,
        "statements": 100
      }
    },
    "setupFiles": [
      "./test.setup.js"
    ]
  }
}
