{
  "name": "react-easy-validation",
  "version": "1.0.3",
  "description": "A simple form validator for React",
  "keywords": [
    "react",
    "validation",
    "validate",
    "validator",
    "form"
  ],
  "main": "dist/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "./node_modules/.bin/jest",
    "watch": "cross-env NODE_ENV=development ./node_modules/.bin/webpack --watch",
    "build": "cross-env NODE_ENV=production ./node_modules/.bin/webpack",
    "prepublishOnly": "npm test && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mattp94/react-easy-validation.git"
  },
  "author": "Matthieu Puibaraud <matthieu.puibaraud@isep.fr> (https://github.com/mattp94)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mattp94/react-easy-validation/issues"
  },
  "homepage": "https://github.com/mattp94/react-easy-validation#readme",
  "devDependencies": {
    "babel-loader": "^7.1.2",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "coveralls": "^2.13.1",
    "cross-env": "^5.0.5",
    "enzyme": "^2.9.1",
    "jest": "^21.1.0",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-test-renderer": "^15.6.1",
    "webpack": "^3.6.0"
  },
  "peerDependencies": {
    "react": "^0.14.0 || ^15.0.0"
  },
  "dependencies": {
    "prop-types": "^15.5.10",
    "shallowequal": "^1.0.2"
  },
  "jest": {
    "verbose": true,
    "collectCoverage": true,
    "coverageThreshold": {
      "global": {
        "branches": 100,
        "functions": 100,
        "lines": 100,
        "statements": 100
      }
    }
  }
}
